{
  "name": "vgs-satellite-client",
  "version": "1.0.0",
  "main": "index.js",
  "keywords": [],
  "author": "VGS",
  "license": "ISC",
  "scripts": {
    "clean": "rm -rf dist/ && rm -rf .cache/",
    "dev": "NODE_ENV=dev parcel src/index.html --open",
    "build": "parcel build src/index.html",
    "serve": "static-server --port 1234 dist",
    "start": "npm run build && npm run serve",
    "cy:dev": "cypress open",
    "cy:run": "cypress run -b chrome -s './cypress/e2e/*.*' --headless",
    "test": "start-test 1234 cy:run"
  },
  "alias": {
    "src": "./src"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "deep-replace-in-object": "^1.1.7",
    "deep-sort-object": "^1.0.2",
    "fast-xml-parser": "^3.16.0",
    "file-saver": "^2.0.2",
    "izitoast": "^1.4.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "prismjs": "^1.19.0",
    "qs": "^6.9.1",
    "react": "^16.13.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-diff-viewer": "^3.0.1",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-treebeard": "^3.2.4",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "url-parse": "^1.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@cypress/code-coverage": "^2.0.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/classnames": "^2.2.9",
    "@types/file-saver": "^2.0.1",
    "@types/js-yaml": "^3.12.2",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.9.0",
    "@types/qs": "^6.9.1",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/reactstrap": "^8.4.2",
    "@types/url-parse": "^1.4.3",
    "antd": "^3.23.5",
    "babel-plugin-istanbul": "^6.0.0",
    "bootstrap": "^4.4.1",
    "cypress": "^4.3.0",
    "cypress-file-upload": "^4.0.4",
    "less": "^3.11.1",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "reactstrap": "^8.4.1",
    "sass": "^1.26.2",
    "source-map-support": "^0.5.16",
    "start-server-and-test": "^1.10.11",
    "static-server": "^2.2.1",
    "typescript": "^3.8.3"
  },
  "browserslist": [
    "since 2017-06"
  ],
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "include": [
      "src/components/**/*"
    ],
    "reporter": [
      "html",
      "text-summary"
    ]
  }
}
